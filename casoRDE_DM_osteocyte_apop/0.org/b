FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField	uniform 0;


boundaryField
{
    INNER
    {
        type            fixedGradient;
	gradient	uniform 0;
    }

    OUTER
    {
        type            zeroGradient;

    }

    BOTTOM
    {
        type            zeroGradient;
	
    }
    
    WALL_INNER
    {
        type            fixedGradient;
	gradient	uniform 0;
    }

    WALL_OUTER
    {
        type            fixedGradient;
	gradient	uniform 0;
    }
}

