FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Dd_v;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-14;


boundaryField
{
    INNER
    {
        type                calculated;
        value               uniform 1e-14;

    }

    OUTER
    {
        type                calculated;
        value               uniform 1e-14;


    }

    BOTTOM
    {
        type                calculated;
        value               uniform 1e-14;

    }
     
        WALL_INNER
    {
        type                calculated;
        value               uniform 1e-14;

    }

    WALL_OUTER
    {
        type                calculated;
        value               uniform 1e-14;

    }
}

