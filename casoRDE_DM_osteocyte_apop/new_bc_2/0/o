FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      o;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;


boundaryField
{
    INNER
    {
        type            zeroGradient;
    }

    OUTER
    {
        type            zeroGradient;

    }

    BOTTOM
    {
        type            zeroGradient;
    }

    WALL_INNER
    {
        type            zeroGradient;
    }
    
     WALL_OUTER
    {
        type            zeroGradient;
    }
}

